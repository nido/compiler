+ **************************************************
+ ST220 Format Database
+ **************************************************

+ ********************* INT3 ***********************
+=INT3
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=00
+field3 I
+field4 CMP=0
+field5 INT3_OPCODE

+ ********************* Int3R **********************
+=Int3R
+type INT3
+format s.0000ooooo...ddddddxxxxxxwwwwww
+syntax %1 = %2, %3
+op1 dest
+op2 src1
+op3 src2

+ ********************* Int3I **********************
+=Int3I
+type INT3
+format s.0010oooooiiiiiiiiiddddddwwwwww
+syntax %1 = %2, %3
+op1 idest
+op2 src1
+op3 isrc2

+ ********************* CMP3 ***********************
+=CMP3
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=00
+field3 I
+field4 CMP=1
+field5 B
+field6 CMP3_OPCODE

+ ********************* Cmp3R_Reg ******************
+=Cmp3R_Reg
+type CMP3
+format s.00010oooo...ddddddxxxxxxwwwwww
+syntax %1 = %2, %3
+op1 dest
+op2 src1
+op3 src2

+ ********************* Cmp3R_Br *******************
+=Cmp3R_Br
+type CMP3
+format s.00011oooobbb......xxxxxxwwwwww
+syntax %1 = %2, %3
+op1 bdest
+op2 src1
+op3 src2

+ ********************* Cmp3I_Reg ******************
+=Cmp3I_Reg
+type CMP3
+format s.00110ooooiiiiiiiiiddddddwwwwww
+syntax %1 = %2, %3
+op1 idest
+op2 src1
+op3 isrc2

+ ********************* Cmp3I_Br *******************
+=Cmp3I_Br
+type CMP3
+format s.00111ooooiiiiiiiii...bbbwwwwww
+syntax %1 = %2, %3
+op1 ibdest
+op2 src1
+op3 isrc2

+ ********************* IMMX ***********************
+=IMMX
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=01
+field3 IMMX_OPCODE

+ ********************* Imm ************************
+=Imm
+type IMMX
+format s.01oooooiiiiiiiiiiiiiiiiiiiiiii
+op1 imm
+syntax %1

+ ********************* SLCT ***********************
+=SLCT
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=01
+field3 I
+field4 SLCT_OPCODE

+ ********************* SelectR ********************
+=SelectR
+type SLCT
+format s.010oooccc...ddddddxxxxxxwwwwww
+syntax %1 = %2, %3, %4
+op1 dest
+op2 scond
+op3 src1
+op4 src2

+ ********************* SelectI ********************
+=SelectI
+type SLCT
+format s.011oooccciiiiiiiiiddddddwwwwww
+syntax %1 = %2, %3, %4
+op1 idest
+op2 scond
+op3 src1
+op4 isrc2

+ ********************* CGEN ***********************
+=CGEN
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=01
+field3 CGEN_OPCODE

+ ********************* cgen ***********************
+=cgen
+type CGEN
+format s.01oooocccbbbddddddxxxxxxwwwwww
+op1 dest
+op2 bdest
+op3 src1
+op4 src2
+op5 scond
+syntax %1, %2 = %3, %4, %5

+ ********************* SYSOP ***********************
+=SYSOP
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=01
+field3 SYSOP_OPCODE

+ ********************* SysOp ***********************
+=SysOp
+type SYSOP
+format s.01ooooooo.....................

+ ********************* LOAD ***********************
+=LOAD
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=10
+field3 LOAD_OPCODE
+field4 D
+field5 S
+field6 L

+ ********************* LOAD ***********************
+=Load
+type LOAD
+format s.10oooo...iiiiiiiiiddddddwwwwww
+syntax %1 = %2[%3]
+op1 idest
+op2 isrc2
+op3 src1

+ ********************* STORE **********************
+=STORE
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=10
+field3 STORE_OPCODE
+field4 S
+field5 L

+ ********************* Store **********************
+=Store
+type STORE
+format s.10ooooo..iiiiiiiiixxxxxxwwwwww
+syntax %1[%2] = %3
+op1 isrc2
+op2 src1
+op3 src2

+ ********************* CALL ***********************
+=CALL
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=11
+field3 I=0
+field4 CALL_OPCODE
+field5 LNK

+ ********************* Call ***********************
+=Call
+type CALL
+format s.110ooo.iiiiiiiiiiiiiiiiiiiiiii
+syntax LR = %1
+op1 btarg

+ ********************* BRANCH *********************
+=BRANCH
+field0 STOPBIT
+field1 CLUSTERBIT=0
+field2 FMT=11
+field3 I=1
+field4 BRANCH_OPCODE

+ ********************* Branch *********************
+=Branch
+type BRANCH
+format s.111occciiiiiiiiiiiiiiiiiiiiiii
+syntax LR = %1
+op1 bcond
+op2 btarg
+syntax %1, %2
