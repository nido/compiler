+ ********************* Load ***********************
+=Load
+field0 STOPBIT
+field1 RESERVED_30_1
+field2 FMT=10
+field3 LOAD_OPCODE
+field4 D
+field5 RESERVED_21_2
+op1 idest
+op2 isrc2
+op3 src1
+syntax %1 = %2[%3]
+type LOAD

+ ********************* Store **********************
+=Store
+field0 STOPBIT
+field1 RESERVED_30_1
+field2 FMT=10
+field3 STORE_OPCODE
+field4 RESERVED_21_2
+op1 isrc2
+op2 src1
+op3 src2
+syntax %1[%2] = %3
+type STORE

+ ********************* Call ***********************
+=Call
+field0 STOPBIT
+field1 RESERVED_30_1
+field2 FMT=11
+field3 I=0
+field4 CALL_OPCODE
+field5 LNK
+op1 btarg
+syntax LR = %1
+type CALL

+ ********************* Branch *********************
+=Branch
+field0 STOPBIT
+field1 RESERVED_30_1
+field2 FMT=11
+field3 I=1
+field4 BRANCH_OPCODE
+op1 bcond
+op2 btarg
+syntax %1, %2
+type BRANCH

