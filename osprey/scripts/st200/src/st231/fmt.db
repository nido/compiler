+=SelectR
+field0 STOPBIT
+field1 RESERVED_30_1
+field2 FMT=01
+field3 I=0
+field4 SLCT_OPCODE
+field5 RESERVED_18_3
+op1 dest
+op2 scond
+op3 src1
+op4 src2
+syntax %1 = %2, %3, %4
