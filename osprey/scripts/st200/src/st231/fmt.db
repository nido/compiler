+ **************************************************
+ ST230 Format Database
+ **************************************************

+ ********************* Int3R **********************
+=Int3R
+field0 STOPBIT
+field1 RESERVED_30_1
+field2 FMT=00
+field3 I=0
+field4 CMP=0
+field5 INT3_OPCODE
+field6 RESERVED_18_3
+op1 dest
+op2 src1
+op3 src2
+syntax %1 = %2, %3
+type INT3

+ ********************* Int3I **********************
+=Int3I
+field0 STOPBIT
+field1 RESERVED_30_1
+field2 FMT=00
+field3 I=1
+field4 CMP=0
+field5 INT3_OPCODE
+op1 idest
+op2 src1
+op3 isrc2
+syntax %1 = %2, %3
+type INT3

+ ********************* Monadic ********************
+=Monadic
+field0 STOPBIT
+field1 RESERVED_30_1
+field2 FMT=00
+field3 I=1
+field4 CMP=0
+field5 INT3_OPCODE=01110
+field6 MONADIC_OPCODE
+op1 idest
+op2 src1
+syntax %1 = %2
+type INT3

