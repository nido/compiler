proc_properties_gen.bo: proc_properties_gen.cxx proc_properties_gen.h targ_proc.h
	$(BUILD_CXX) $(BUILD_CXXFLAGS) -c $< -o $@

proc_properties.bo: proc_properties.cxx proc_properties_gen.h targ_proc.h
	$(BUILD_CXX) $(BUILD_CXXFLAGS) -c $< -o $@

si_gen.bo: si_gen.cxx si_gen.h topcode.h targ_isa_properties.h \
          targ_isa_subset.h targ_isa_operands.h
	$(BUILD_CXX) $(BUILD_CXXFLAGS) -c $< -o $@

ifeq ($(BUILD_COMPILER), EDG)
si_gen.so: si_gen.cxx topcode.h si_gen.h targ_isa_operands.h \
	targ_isa_subset.o targ_isa_properties.o
	$(CXX) $(CXXLDFLAGS) $(TARGINFO_GENERATE_DIR)/si_gen.cxx \
	targ_isa_subset.o targ_isa_properties.o \
	-shared -o si_gen.so -rpath $(CWD)
else
si_gen.so: si_gen.bo topcode.h si_gen.h targ_isa_operands.h \
        targ_isa_operands.bo targ_isa_properties.bo targ_isa_registers.bo \
	targ_isa_subset.bo targ_isa_enums.bo targ_isa_lits.bo
	$(CXX) $(CXXLDFLAGS) si_gen.bo \
	targ_isa_subset.bo targ_isa_properties.bo targ_isa_operands.bo \
	targ_isa_registers.bo targ_isa_enums.bo targ_isa_lits.bo \
	-shared -o $@

si_gen.a: si_gen.bo topcode.h si_gen.h targ_isa_operands.h \
        targ_isa_operands.bo targ_isa_properties.bo targ_isa_registers.bo \
	targ_isa_subset.bo targ_isa_enums.bo targ_isa_lits.bo
	$(BUILD_AR) rc $@ si_gen.bo \
	targ_isa_subset.bo targ_isa_properties.bo targ_isa_operands.bo \
	targ_isa_registers.bo targ_isa_enums.bo targ_isa_lits.bo 
endif

# list of files that do not require a specific rule
MISC_HOST_OBJS = targ_isa_subset.ho \
		 targ_isa_properties.ho \
		 targ_isa_enums.ho \
		 targ_isa_hazards.ho \
		 targ_isa_lits.ho \
		 targ_isa_print.ho \
		 targ_isa_pack.ho \
		 targ_isa_operands.ho \
		 targ_isa_registers.ho \
		 targ_isa_decode.ho \
		 targ_isa_pseudo.ho \
		 targ_abi_properties.ho \
		 targ_isa_bundle.ho \
		 targ_proc.ho \
		 targ_proc_properties.ho

MISC_BUILD_OBJS = targ_isa_subset.bo \
	    	  targ_isa_registers.bo \
		  targ_isa_enums.bo \
		  targ_isa_lits.bo \
		  targ_isa_properties.bo \
		  targ_isa_bundle.bo

$(MISC_BUILD_OBJS) : %.bo : %.c %.h
	$(BUILD_CC) $(BUILD_CFLAGS) $< -c -o $@

$(MISC_HOST_OBJS) : %.ho : %.c %.h
	$(CC) $(CFLAGS) $< -c -o $@


