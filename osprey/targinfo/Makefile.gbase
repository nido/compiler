	$(CC) $(CFLAGS) $(STD_DSO_LOADOPTS) $< -o $@

#
# windows dll (+ import library)
#
st220.dll st220_dll.a : st220.ho st200_DllMain.ho $(LIBTARGINFO_OBJS)
	$(CC) --shared -o st220.dll -Wl,--out-implib,st220_dll.a \
	$^ $(LDDSOOPTS) -Wl,--image-base=0x12000000

#
# static library
#
libst220.a : st220.ho
	$(AR) rc $@ $<

