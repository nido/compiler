DEPS_dyn_isa_variants.h  = dyn_isa_variants.h $(EXTN)_targ_isa_variants.h

###############################
## xxx_targ_isa_print.[ch]   ##
###############################

$(EXTN)_isa_syntax_gen.$(BOBJ): isa_syntax_gen.cxx isa_syntax_gen.h $(EXTN)_topcode.h 	\
				gen_util.h $(EXTN)_targ_isa_operands.h 			\
				$(EXTN)_targ_isa_registers.h $(EXTN)_targ_isa_lits.h 	\
				$(EXTN)_targ_isa_subset.h
	$(META)$(BUILD_CXX) $(BUILD_CXXFLAGS) $(COMPFLAG) $< $(OUTOBJFLAG)$@

$(EXTN)_isa_syntax.$(BOBJ): $(EXTN)_isa_syntax.cxx isa_syntax_gen.h $(EXTN)_topcode.h 	\
			    $(EXTN)_targ_isa_registers.h $(EXTN)_targ_isa_lits.h 	\
			    $(EXTN)_targ_isa_subset.h
	$(META)$(BUILD_CXX) $(BUILD_CXXFLAGS) $(COMPFLAG) $< $(OUTOBJFLAG)$@

$(EXTN)_isa_syntax_gen$(BEXE): $(EXTN)_isa_syntax_gen.$(BOBJ)                  	\
                              $(EXTN)_isa_syntax.$(BOBJ)                        \
                              $(EXTN)_gen_util.$(BOBJ)                         	\
			      $(EXTN)_stub_isa_topcode.$(BOBJ)		  	\
			      $(EXTN)_stub_isa_subset.$(BOBJ)		  	\
			      $(EXTN)_stub_isa_registers.$(BOBJ)	  	\
			      $(EXTN)_stub_isa_lits.$(BOBJ)	  		\
			      $(EXTN)_stub_isa_enums.$(BOBJ)		  	\
			      $(EXTN)_stub_isa_relocs.$(BOBJ)		  	\
			      $(EXTN)_stub_isa_operands.$(BOBJ)		  	\
                              $(EXTN)_$(BUILD_MANGLING)_topcode.$(BOBJ)        	\
                              $(EXTN)_$(BUILD_MANGLING)_isa_subset.$(BOBJ)  	\
                              $(EXTN)_$(BUILD_MANGLING)_isa_operands.$(BOBJ)  	\
                              $(EXTN)_$(BUILD_MANGLING)_isa_registers.$(BOBJ)  	\
                              $(EXTN)_$(BUILD_MANGLING)_isa_lits.$(BOBJ)  	\
                              $(EXTN)_$(BUILD_MANGLING)_isa_relocs.$(BOBJ)  	\
                              $(EXTN)_$(BUILD_MANGLING)_isa_enums.$(BOBJ)  	\
                              $(EXTN)_$(BUILD_MANGLING)_isa_properties.$(BOBJ)	\
                              $(ISA_CORE_REGISTER)
	$(META)$(BUILD_CXX) $^ $(OUTEXEFLAG)$@ $(BUILD_CXXLDFLAGS)

$(EXTN)_targ_isa_parse.c: $(EXTN)_targ_isa_parse.h
	@$(REM_WIN32)# $@: force dependency
$(EXTN)_targ_isa_parse.h: $(EXTN)_targ_isa_print.h
	@$(REM_WIN32)# $@: force dependency
$(EXTN)_targ_isa_print.c: $(EXTN)_targ_isa_print.h
	@$(REM_WIN32)# $@: force dependency
$(EXTN)_subset_targ_isa_parse.pr: $(EXTN)_targ_isa_print.h
	@$(REM_WIN32)# $@: force dependency
$(EXTN)_targ_isa_print.h: $(EXTN)_isa_syntax_gen$(BEXE)
	@echo "  -Building $@"
	$(META)$(RUN) ./$<

$(EXTN)_$(HOST_MANGLING)_isa_print.$(BOBJ): $(EXTN)_targ_isa_print.c $(EXTN)_targ_isa_print.h
	$(META)$(CC) $(CFLAGS) $(COMPFLAG) $< $(OUTOBJFLAG)$@

$(EXTN)_$(HOST_MANGLING)_targ_isa_parse.$(BOBJ): 	$(EXTN)_targ_isa_parse.c $(EXTN)_targ_isa_parse.h
	$(META)$(CC) $(CFLAGS) $(COMPFLAG) $< $(OUTOBJFLAG)$@

$(EXTN)_$(HOST_MANGLING)_isa_parse.$(BOBJ): 	$(EXTN)_subset_targ_isa_parse.c
	$(META)$(CC) $(CFLAGS) $(COMPFLAG) $< $(OUTOBJFLAG)$@

$(EXTN)_subset_targ_isa_parse.c: $(EXTN)_subset_targ_isa_parse.pr $(BUILD_AREA)/gensyn/gensyn$(BEXE)
