$(EXTN)_$(BUILD_MANGLING)_isa_variants.$(BOBJ) : $(EXTN)_targ_isa_variants.c
	$(META)$(BUILD_CC) $(BUILD_CFLAGS) $(COMPFLAG) $< $(OUTOBJFLAG)$@
