	$(META)$(BUILD_CXX) $(BUILD_CXXFLAGS) $(COMPFLAG) $< $(OUTOBJFLAG)$@

###############################
## xxx_isa_variants.[ch]   ##
###############################

$(EXTN)_isa_variants_gen.$(BOBJ): isa_variants_gen.cxx \
                              isa_variants_gen.h   \
