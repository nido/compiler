$(EXTN)_targ_isa_variants.c: $(EXTN)_targ_isa_variants.h
	@$(REM_WIN32)# $@: force dependency
$(EXTN)_targ_isa_variants.h: $(EXTN)_isa_variants_gen$(BEXE)
	@echo "  -Building $@"
	$(META)$(RUN) ./$<

